insert into user_role (user_id, role_id) values (
(select id from uporabniki where username='admin'),
(select id from role where name = 'prijavljen-uporabnik')

)




SELECT 
  cr.id, 
  uporabniki.username, 
  cr.name, 
  hr.name
FROM 
  public.uporabniki, 
  public.user_hierroles, 
  public.user_role, 
  public.role hr, 
  public.role cr
WHERE 
  user_hierroles.role_id = hr.id AND
  user_hierroles.user_id = uporabniki.id AND
  user_role.user_id = uporabniki.id AND
  cr.id = user_role.role_id;
